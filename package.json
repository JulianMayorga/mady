{
  "name": "mady",
  "version": "0.1.0",
  "description": "Easy-to-use MessageFormat translator tool",
  "bin": {
    "mady": "lib/es5/index.js"
  },
  "engines": {
    "node": ">=4"
  },
  "author": "Guillermo Grau Panea",
  "license": "MIT",
  "keywords": [
    "i18n",
    "MessageFormat",
    "translation",
    "locales",
    "translator"
  ],
  "homepage": "https://github.com/guigrpa/mady#readme",
  "bugs": {
    "url": "https://github.com/guigrpa/mady/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guigrpa/mady.git"
  },
  "scripts": {
    "start": "babel-node src/index.js",
    "compile": "rm -rf ./lib && mkdir lib && babel -d lib/es5 src && babel --no-babelrc --plugins transform-flow-strip-types -d lib/es6 src && cp -r src lib/es6_flow",
    "docs": "extract-docs --template docs/templates/README.md --output README.md",
    "build": "npm run lint && npm run flow && npm run compile && npm run test && npm run docs",
    "travis": "npm run compile && npm run testCovFullExceptMin",
    "lint": "eslint src",
    "flow": "flow && test $? -eq 0 -o $? -eq 2",
    "flowStop": "flow stop",
    "test": "npm run testCovFull",
    "testCovFull": "npm run testCovPrepare && npm run testDev && npm run testProd && npm run testCovReport",
    "testCovFullExceptMin": "npm run testCovPrepare && npm run testDev && npm run testProd && npm run testCovReport",
    "testCovFast": "npm run testCovPrepare && npm run testDev && npm run testCovReport",
    "ava": "ava --watch",
    "testCovPrepare": "rm -rf ./coverage .nyc_output .nyc_tmp && mkdir .nyc_tmp",
    "testDev": "cross-env NODE_ENV=development nyc ava && mv .nyc_output/* .nyc_tmp/",
    "testProd": "cross-env NODE_ENV=production nyc ava && mv .nyc_output/* .nyc_tmp/",
    "testMin": "cross-env TEST_MINIFIED_LIB=1 nyc ava && mv .nyc_output/* .nyc_tmp/",
    "testCovReport": "cp .nyc_tmp/* .nyc_output/ && nyc report --reporter=html --reporter=lcov --reporter=text"
  },
  "dependencies": {
    "bluebird": "^3.3.4",
    "commander": "^2.9.0",
    "express": "4.13.4",
    "fs-extra": "^0.26.7",
    "inquirer": "^0.12.0",
    "storyboard-core": "^1.0.0",
    "timm": "^0.6.0"
  },
  "devDependencies": {
    "ava": "^0.13.0",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-eslint": "^6.0.0",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-flow-strip-types": "^6.7.0",
    "babel-polyfill": "6.6.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-relay-plugin": "0.7.3",
    "coveralls": "2.11.6",
    "cross-env": "^1.0.7",
    "css-loader": "0.23.1",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-flowtype": "^2.2.2",
    "eslint-plugin-react": "^4.2.3",
    "express-graphql": "0.4.9",
    "extract-docs": "^1.0.0",
    "file-loader": "0.8.5",
    "flow-bin": "^0.22.1",
    "font-awesome": "4.5.0",
    "graphql": "0.4.17",
    "graphql-relay": "0.3.6",
    "moment": "^2.11.2",
    "node-sass": "3.4.2",
    "nyc": "^6.1.1",
    "react": "15.0.0-rc.1",
    "react-addons-perf": "15.0.0-rc.1",
    "react-addons-pure-render-mixin": "15.0.0-rc.1",
    "react-dom": "15.0.0-rc.1",
    "react-relay": "0.7.3",
    "sass-loader": "3.1.2",
    "style-loader": "0.13.0",
    "tinycolor2": "1.3.0",
    "webpack": "1.12.13",
    "webpack-dev-middleware": "1.5.1",
    "webpack-hot-middleware": "2.6.4"
  },
  "ava": {
    "files": [
      "./test/test.js"
    ],
    "babel": "inherit"
  }
}
