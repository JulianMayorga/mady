{
  "name": "mady",
  "version": "1.3.1",
  "description": "Easy-to-use tool to manage and translate ICU MessageFormat messages",
  "bin": {
    "mady": "lib/mady.js"
  },
  "main": "lib/translate.js",
  "engines": {
    "node": ">=4"
  },
  "author": "Guillermo Grau Panea",
  "license": "MIT",
  "keywords": [
    "i18n",
    "MessageFormat",
    "translation",
    "locales",
    "translator"
  ],
  "homepage": "https://github.com/guigrpa/mady#readme",
  "bugs": {
    "url": "https://github.com/guigrpa/mady/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guigrpa/mady.git"
  },
  "scripts": {
    "start": "babel-node src/server/startup --dir src/locales",
    "startAlt": "babel-node src/server/startup --dir tools/locales",
    "startProd": "node lib/mady",
    "compile": "rm -rf ./lib && mkdir lib && babel -d lib src",
    "updateSchemaJson": "babel-node src/server/gqlUpdateSchema",
    "docs": "extract-docs --template docs/templates/README.md --output README.md",
    "buildSsrWatch": "rm -rf ./public/ssr && rm -rf ./lib/server/ssr && cross-env SERVER_SIDE_RENDERING=true webpack --config ./src/server/webpackConfig.cjs --progress --display-chunks --watch",
    "buildSsr": "rm -rf ./public/ssr && rm -rf ./lib/server/ssr && cross-env SERVER_SIDE_RENDERING=true NODE_ENV=production webpack --config ./src/server/webpackConfig.cjs --progress --display-chunks",
    "buildClient": "rm -rf ./public/assets && cross-env NODE_ENV=production webpack --config ./src/server/webpackConfig.cjs --progress --display-chunks",
    "build": "node package && npm run compile && npm run buildClient && npm run buildSsr && npm run docs && npm run xxl",
    "travis": "npm run compile && npm run testCovFullExceptMin",
    "lint": "eslint src",
    "flow": "flow && test $? -eq 0 -o $? -eq 2",
    "flowStop": "flow stop",
    "xxl": "xxl --src \"[\\\"src\\\"]\"",
    "test": "npm run testCovFull",
    "testCovFull": "npm run testCovPrepare && npm run testDev && npm run testProd && npm run testCovReport",
    "testCovFullExceptMin": "npm run testCovPrepare && npm run testDev && npm run testProd && npm run testCovReport",
    "testCovFast": "npm run testCovPrepare && npm run testDev && npm run testCovReport",
    "ava": "ava --watch",
    "testCovPrepare": "rm -rf ./coverage .nyc_output .nyc_tmp && mkdir .nyc_tmp",
    "testDev": "cross-env NODE_ENV=development nyc ava && mv .nyc_output/* .nyc_tmp/",
    "testProd": "cross-env NODE_ENV=production nyc ava && mv .nyc_output/* .nyc_tmp/",
    "testMin": "cross-env TEST_MINIFIED_LIB=1 nyc ava && mv .nyc_output/* .nyc_tmp/",
    "testCovReport": "cp .nyc_tmp/* .nyc_output/ && nyc report --reporter=html --reporter=lcov --reporter=text"
  },
  "dependencies": {
    "babel-relay-plugin": "0.9.2",
    "bluebird": "3.4.1",
    "commander": "2.9.0",
    "compression": "1.6.2",
    "cookie-parser": "1.4.3",
    "diveSync": "0.3.0",
    "ejs": "2.4.2",
    "express": "4.14.0",
    "express-graphql": "0.5.3",
    "fs-extra": "0.30.0",
    "graphql": "0.6.1",
    "graphql-relay": "0.4.2",
    "inquirer": "1.1.2",
    "lodash": "4.14.1",
    "messageformat": "0.3.0",
    "node-uuid": "1.4.7",
    "slash": "1.0.0",
    "storyboard": "2.0.2",
    "timm": "1.0.0",
    "uglify-js": "2.6.2"
  },
  "devDependencies": {
    "ava": "0.15.0",
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-eslint": "6.0.4",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-flow-strip-types": "6.8.0",
    "babel-polyfill": "6.7.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.5.0",
    "bundle-loader": "0.5.4",
    "coveralls": "2.11.11",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "eslint": "2.9.0",
    "eslint-config-airbnb": "9.0.1",
    "eslint-plugin-flowtype": "2.3.1",
    "eslint-plugin-import": "1.8.0",
    "eslint-plugin-jsx-a11y": "1.2.2",
    "eslint-plugin-react": "5.2.2",
    "extract-docs": "1.0.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "flow-bin": "0.23.1",
    "font-awesome": "4.6.3",
    "giu": "0.7.1",
    "isomorphic-relay": "0.7.0",
    "json-loader": "0.5.4",
    "moment": "2.13.0",
    "node-sass": "3.7.0",
    "nyc": "6.4.4",
    "react": "15.2.1",
    "react-addons-perf": "15.2.1",
    "react-addons-pure-render-mixin": "15.2.1",
    "react-dom": "15.2.1",
    "react-relay": "0.9.2",
    "sass-loader": "3.2.0",
    "style-loader": "0.13.1",
    "tiny-cookie": "0.5.5",
    "tinycolor2": "1.3.0",
    "webpack": "1.13.1",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0",
    "xxl": "0.1.1"
  },
  "ava": {
    "files": [
      "./test/test.js"
    ],
    "babel": "inherit"
  }
}
