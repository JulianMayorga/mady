{
  "name": "mady",
  "version": "0.1.3",
  "description": "Easy-to-use tool to manage and translate ICU MessageFormat messages",
  "bin": {
    "mady": "lib/mady.js"
  },
  "main": "lib/translate.js",
  "engines": {
    "node": ">=4"
  },
  "author": "Guillermo Grau Panea",
  "license": "MIT",
  "keywords": [
    "i18n",
    "MessageFormat",
    "translation",
    "locales",
    "translator"
  ],
  "homepage": "https://github.com/guigrpa/mady#readme",
  "bugs": {
    "url": "https://github.com/guigrpa/mady/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guigrpa/mady.git"
  },
  "scripts": {
    "start": "babel-node src/server/startup",
    "startProd": "cross-env NODE_ENV=production node lib/server/startup",
    "compile": "rm -rf ./lib && mkdir lib && babel -d lib src",
    "updateSchemaJson": "babel-node src/server/gqlUpdateSchema",
    "docs": "extract-docs --template docs/templates/README.md --output README.md",
    "buildSsrWatch": "rm -rf ./lib/server/ssr && cross-env SERVER_SIDE_RENDERING=true webpack --config ./src/server/webpackConfigCommonJS --progress --display-chunks --watch",
    "buildSsr": "rm -rf ./lib/server/ssr && cross-env SERVER_SIDE_RENDERING=true webpack --config ./src/server/webpackConfigCommonJS --progress --display-chunks",
    "buildClient": "cross-env NODE_ENV=production webpack --config ./src/server/webpackConfigCommonJS --progress --display-chunks",
    "build": "node package && npm run compile && npm run buildClient && npm run test && npm run docs",
    "travis": "npm run compile && npm run testCovFullExceptMin",
    "lint": "eslint src",
    "flow": "flow && test $? -eq 0 -o $? -eq 2",
    "flowStop": "flow stop",
    "test": "npm run testCovFull",
    "testCovFull": "npm run testCovPrepare && npm run testDev && npm run testProd && npm run testCovReport",
    "testCovFullExceptMin": "npm run testCovPrepare && npm run testDev && npm run testProd && npm run testCovReport",
    "testCovFast": "npm run testCovPrepare && npm run testDev && npm run testCovReport",
    "ava": "ava --watch",
    "testCovPrepare": "rm -rf ./coverage .nyc_output .nyc_tmp && mkdir .nyc_tmp",
    "testDev": "cross-env NODE_ENV=development nyc ava && mv .nyc_output/* .nyc_tmp/",
    "testProd": "cross-env NODE_ENV=production nyc ava && mv .nyc_output/* .nyc_tmp/",
    "testMin": "cross-env TEST_MINIFIED_LIB=1 nyc ava && mv .nyc_output/* .nyc_tmp/",
    "testCovReport": "cp .nyc_tmp/* .nyc_output/ && nyc report --reporter=html --reporter=lcov --reporter=text"
  },
  "dependencies": {
    "bluebird": "^3.3.4",
    "commander": "^2.9.0",
    "compression": "1.6.1",
    "cookie-parser": "1.4.1",
    "diveSync": "0.3.0",
    "ejs": "2.4.1",
    "express": "4.13.4",
    "fs-extra": "^0.26.7",
    "inquirer": "^1.0.0",
    "lodash": "^4.8.2",
    "messageformat": "0.3.0",
    "node-uuid": "1.4.7",
    "storyboard": "^1.0.0",
    "timm": "^0.6.0",
    "uglify-js": "2.6.2"
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-eslint": "^6.0.0",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-flow-strip-types": "^6.7.0",
    "babel-polyfill": "6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-relay-plugin": "0.8.0",
    "coveralls": "^2.11.6",
    "cross-env": "^1.0.7",
    "css-loader": "0.23.1",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-flowtype": "^2.2.2",
    "eslint-plugin-jsx-a11y": "^0.6.2",
    "eslint-plugin-react": "^4.3.0",
    "express-graphql": "0.5.1",
    "extract-docs": "^1.0.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "flow-bin": "^0.22.1",
    "font-awesome": "4.5.0",
    "graphql": "0.5.0",
    "graphql-relay": "0.3.6",
    "json-loader": "0.5.4",
    "moment": "^2.11.2",
    "node-sass": "3.4.2",
    "nyc": "^6.1.1",
    "react": "15.0.1",
    "react-addons-perf": "15.0.1",
    "react-addons-pure-render-mixin": "15.0.1",
    "react-dom": "15.0.1",
    "react-relay": "0.8.0",
    "sass-loader": "3.2.0",
    "style-loader": "0.13.1",
    "tinycolor2": "1.3.0",
    "webpack": "1.12.15",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0"
  },
  "ava": {
    "files": [
      "./test/test.js"
    ],
    "babel": "inherit"
  }
}
